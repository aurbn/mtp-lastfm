<?xml version="1.0" encoding="UTF-8"?>
<mtp-lastfm status="{{status}}">
    {% ifequal status "OK" %}
    <device>
      <manufacturer>{{device.manufacturer}}</manufacturer>
      <model>{{device.model}}</model>
      <users>{{device.users}}</users>
      <issues>{{device.issues}}</issues>
    </device>
  {% else %}
  <error>{{error_msg}}</error>
  {% endifequal %}
</mtp-lastfm>